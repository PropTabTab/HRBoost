@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

@model HRBoost.Entity.Document

@{
    ViewData["Title"] = "Add";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}



<h4>Özlük Ekle </h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Add" method="post" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            
            
            <div class="form-group">
                <label asp-for="FileName" class="control-label">Dosya Adı</label>
                <input asp-for="FileName" class="form-control" />
                <span asp-validation-for="FileName" class="text-danger"></span>
          
            
            <div class="form-group">
                <div>
                    <label for="file">Dosya Yükleyin:</label>
                    <input asp-for=File type="file" name="file" id="file" />
                </div>

          

            <div class="form-group">
                <label asp-for="UploadDate" class="control-label">Tarih</label>
                <input asp-for="UploadDate" class="form-control" />
                <span asp-validation-for="UploadDate" class="text-danger"></span>
            </div>

            <div class="form-group">
                <input type="submit" value="Kaydet" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Listeye Dön</a>
</div>

